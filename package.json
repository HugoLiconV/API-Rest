{
		"name": "api-rest",
		"version": "1.0.0",
		"description": "API REST con NodeJS y Express",
		"private": true,
		"main": "src",
		"scripts": {
				"start": "node .",
				"dev": "nodemon -i \"*.test.js\" .",
				"prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
				"lint": "eslint src",
				"docs": "apidoc -i src -o docs && apidoc-markdown2 -p docs -o DOCS.md",
				"postdocs": "opn ./docs/index.html",
				"test": "jest --colors",
				"test:articles": "npm test src/api/article/index\".test\".js$",
				"test:users": "npm test src/api/user/index\".test\".js$",
				"test:unit": "npm test \"src/**/!(index).test.js\"",
				"test:integration": "npm test \"src/**/index.test.js\"",
				"coverage": "npm test -- --coverage",
				"postcoverage": "opn coverage/lcov-report/index.html"
		},
		"jest": {
				"testEnvironment": "node",
				"setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
		},
		"devDependencies": {
				"apidoc": "0.17.6",
				"apidoc-markdown": "0.2.0",
				"babel-eslint": "8.2.2",
				"babel-jest": "18.0.0",
				"cross-env": "3.1.3",
				"dotenv-safe": "4.0.3",
				"eslint": "4.19.1",
				"eslint-config-standard": "11.0.0",
				"eslint-plugin-promise": "3.7.0",
				"eslint-plugin-import": "2.10.0",
				"eslint-plugin-node": "6.0.1",
				"eslint-plugin-standard": "3.0.1",
				"jest-cli": "22.4.3",
				"mockgoose": "7.3.5",
				"nock": "9.0.2",
				"nodemon": "1.17.3",
				"opn-cli": "3.1.0",
				"sinon": "1.17.5",
				"supertest": "2.0.0",
				"supertest-as-promised": "4.0.0"
		},
		"dependencies": {
				"babel-core": "6.26.0",
				"babel-plugin-transform-runtime": "6.23.0",
				"babel-preset-es2015": "6.24.1",
				"babel-preset-stage-1": "6.24.1",
				"babel-runtime": "6.26.0",
				"bcrypt": "1.0.2",
				"bluebird": "3.4.0",
				"body-parser": "1.15.1",
				"bodymen": "1.0.1",
				"chalk": "2.3.2",
				"compression": "1.7.2",
				"cors": "2.8.4",
				"express": "4.13.4",
				"jsonwebtoken": "7.1.9",
				"lodash": "4.17.5",
				"mongoose": "4.7.8",
				"mongoose-create-unique": "0.4.4",
				"mongoose-keywords": "0.3.2",
				"morgan": "1.7.0",
				"passport": "0.3.2",
				"passport-http": "0.3.0",
				"passport-http-bearer": "1.0.1",
				"passport-jwt": "2.1.0",
				"querymen": "2.0.0",
				"rand-token": "0.3.0",
				"request": "2.74.0",
				"request-promise": "4.1.1"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/HugoLiconV/API-Rest.git"
		},
		"author": "",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/HugoLiconV/API-Rest/issues"
		},
		"homepage": "https://github.com/HugoLiconV/API-Rest#readme"
}
